{
  "type": "PTE",
  "templateUrl": "https://github.com/microsoft/AL-Go@bugsbash/v8.0",
  "runs-on": "windows-latest",
  "shell": "pwsh",
  "githubRunner": "ubuntu-latest",
  "githubRunnerShell": "pwsh",
  "UpdateDependencies": true,
  "trackALAlertsInGithub": true,
  "ALDoc": {
    "ContinuousDeployment": true,
    "MaxReleases": 0
  },
  "useCompilerFolder": true,
  "doNotPublishApps": true,
  "doNotSignApps": true,
  "keyVaultCodesignCertificateName": "FreddyKristiansen",
  "templateSha": "444922717a7b006b8b8b7a8c1375b1baf2a2261d",
  "conditionalSettings": [
    {
      "workflows": [
        "Update AL-Go System Files"
      ],
      "settings": {
        "workflowSchedule": {
          "cron": "0 0 * * *"
        }
      }
    },
    {
      "workflows": [
        "Test Current"
      ],
      "settings": {
        "workflowSchedule": {
          "cron": "0 2 * * 1,2,3,4,5"
        }
      }
    },
    {
      "workflows": [
        "Test Next Major"
      ],
      "settings": {
        "workflowSchedule": {
          "cron": "0 2 * * 6"
        }
      }
    },
    {
      "workflows": [
        "Test Next Minor"
      ],
      "settings": {
        "workflowSchedule": {
          "cron": "0 2 * * 6"
        }
      }
    }
  ]
}
